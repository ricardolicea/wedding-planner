{
  "name": "wedding-planner",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "npm run start:dev --workspace api",
    "dev:web": "npm run dev --workspace web",
    "build:api": "npm run build --workspace api",
    "build:web": "npm run build --workspace web",
    "lint:api": "npm run lint --workspace api",
    "lint:web": "npm run lint --workspace web",
    "lint": "eslint . --max-warnings=0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/axios": "^0.9.36",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/config": "^4.0.2",
    "api": "^0.0.1",
    "axios": "^1.13.2",
    "build": "^0.1.4",
    "dotenv": "^17.2.3",
    "npx": "^10.2.2",
    "nx": "^22.1.3"
  }
}
